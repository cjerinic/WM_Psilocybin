        <Param val="# READ-IN AND ASSIGN PRACTICE AND MAIN DESIGN MATRICES&amp;#10;import os as _os&amp;#10;&amp;#10;participant_number = expInfo['participant']&amp;#10;&amp;#10;expDir = _os.path.dirname(_os.path.abspath(__file__))&amp;#10;&amp;#10;practice_list = _os.path.join(expDir, 'subject_designs', f'sub-{participant_number}', 'practice', 'practice_design.csv')&amp;#10;&amp;#10;main_list = _os.path.join(expDir, 'subject_designs', f'sub-{participant_number}', 'main', 'main_design.csv')" valType="extendedCode" updates="constant" name="End Routine"/>
